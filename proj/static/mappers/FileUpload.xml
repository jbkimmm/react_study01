<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="FileUpload">

	<!-- #### 파일이름 저장 #### -->
	<insert id="file" useGeneratedKeys="false"
		parameterType="map">

INSERT INTO test
		(name
		,test
		)
VALUES (#{file}
		,#{test}
		)

	</insert>
	
</mapper>